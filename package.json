{
  "name": "trpc-nestjs-micro",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.6.0",
    "turbo": "^2.5.4",
    "typescript": "5.8.2"
  },
  "pnpm": {
    "overrides": {
      "typescript": "5.8.2"
    },
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "@swc/core"
    ]
  },
  "packageManager": "pnpm@10.12.4",
  "engines": {
    "node": ">=18"
  }
}